doctype html
head
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    title #{ title }
    meta(name='author', content='SOCIS')
    meta(name='description', content='Computer Science We are a group of computer science students for computer science students. We coordinate with the School of Computer Science in â€¦')
    meta(name='viewport', content='width=device-width')
    link(rel='canonical', href='http://UoGSOCIS.github.io')
    link(href='/favicon.png', rel='icon')
    link(href='/stylesheets/screen.css', media='screen, projection', rel='stylesheet')
  body
    != partial("../../_partials/nav.jade")
    .grid-wrapper
      .grid.grid-center
        .grid__item.four-fifths.lap-one-whole.palm-one-whole
